{
  "name": "iParser for Binary.com",
  "description": "this thing can make you rich!",
  "version": "1.1",
  "manifest_version": 2,

  "permissions": ["tabs",
    "<all_urls>",
    "...",
    "fileSystem"
  ],

  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
}